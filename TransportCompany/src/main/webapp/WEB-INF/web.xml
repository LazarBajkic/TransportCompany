<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CheckCred</servlet-name>
  	<display-name>CheckCred</display-name>
  	<description></description>
  	<servlet-class>com.TransportCompany.Login.CheckCred</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RefreshListOfVehicles</servlet-name>
  	<display-name>RefreshListOfVehicles</display-name>
  	<description></description>
  	<servlet-class>com.TransportCompany.RefreshListOfVehicles</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminActions</servlet-name>
  	<display-name>AdminActions</display-name>
  	<description></description>
  	<servlet-class>com.TransportCompany.Admin.AdminActions</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>managerActions</servlet-name>
  	<display-name>managerActions</display-name>
  	<description></description>
  	<servlet-class>com.TransportCompany.Manager.ManagerActions</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserActions</servlet-name>
  	<display-name>UserActions</display-name>
  	<description></description>
  	<servlet-class>com.TransportCompany.User.UserActions</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CheckCred</servlet-name>
  	<url-pattern>/checkCred</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RefreshListOfVehicles</servlet-name>
  	<url-pattern>/refreshList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminActions</servlet-name>
  	<url-pattern>/adminActions</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>managerActions</servlet-name>
  	<url-pattern>/managerActions</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserActions</servlet-name>
  	<url-pattern>/registerDelivery</url-pattern>
  </servlet-mapping>
</web-app>
